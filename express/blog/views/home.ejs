<%-include('partials/header') %> 

<div class="container">
    <h1>Home</h1>
<p><%=startingContent  %> </p> 
</div>


<div class="container">
    <h2>Blogs</h2>
    <div class=" mb-3">
    <% posts.forEach( (element, index) => { %>
        <div class="card mb-3">
        <div class="card-body">
            <h4 class="card-title"><%=element.title%></h4>
            <p class="card-text"><%=element.content.slice(0,100)%>........
                <a href="/post/<%=element._id%>">Read More</a>
            </p>
        </div>
        </div>
     
    <% }) %>
</div>
</div>

<%-include('partials/footer') %> 

